{
  "name": "igodatapackager",
  "version": "1.0.0",
  "description": "This repository contains the IgoDataPackager tool, designed to package igo2 data into zip files.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "rimraf dist && tsc && babel src -d dist --extensions .ts --ignore **/*.spec.ts && node dist/index.js",
    "test": "rimraf dist && cross-env TS_NODE_PROJECT='./tsconfig.spec.json' tsc && cross-env NODE_ENV=test babel src -d dist/ --extensions '.ts' --source-maps inline && mocha dist/**/*.spec.js",
    "coverage": "nyc npm test"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.spec.ts",
      "src/index.ts"
    ],
    "reporter": [
      "html",
      "text",
      "cobertura"
    ],
    "all": true,
    "cache": true,
    "sourceMap": true,
    "instrument": true
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/proj4": "^2.5.5",
    "@types/rbush": "^3.0.3",
    "@types/sinon": "^17.0.3",
    "@types/unzipper": "^0.10.9",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^5.0.3",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.5",
    "sinon": "^17.0.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.9",
    "@supercharge/promise-pool": "^3.1.0",
    "@turf/boolean-intersects": "^6.5.0",
    "@turf/boolean-point-in-polygon": "^6.5.0",
    "@turf/turf": "^6.5.0",
    "@types/adm-zip": "^0.5.5",
    "@types/archiver": "^6.0.2",
    "@types/geojson": "^7946.0.13",
    "@types/uuid": "^9.0.7",
    "adm-zip": "^0.5.10",
    "archiver": "^6.0.1",
    "axios": "^1.6.5",
    "esm": "^3.2.25",
    "ol": "^8.2.0",
    "proj4": "^2.10.0",
    "rbush": "^2.0.2",
    "ts-node": "^10.9.2",
    "unzipper": "^0.10.14",
    "uuid": "^9.0.1"
  }
}
